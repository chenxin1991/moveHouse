page{
  background: #fff
}
.item {
  width: 92%;
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  margin: 0 auto;
  box-sizing: border-box;
}

.item .child {
  text-align: center;
  width:220rpx;
  height:200rpx;
  font-size: 24rpx;
  color: #fff;
  line-height: 1;
  border-radius: 10rpx;
  position: relative;
}


.item>view image {
  width: 100%;
  height: 100%;
}

.item>view view {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
}

.item>view navigator {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}



