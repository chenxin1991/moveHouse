/* 战略合作伙伴 */
.teamwork{
  display: flex;
  justify-content: center;
  flex-wrap:wrap;
  margin-bottom: 15rpx;
}
.teamwork .teamwork-item{
   width: 23%;
   display: flex;
   align-items: center;
   flex-direction: column;
   padding: 20rpx;
   border-top: 2rpx solid #ededed;
   border-bottom: 2rpx solid #ededed;
   border-right: 2rpx solid #ededed;
 }
 .teamwork .teamwork-item-rows{
   border-left:  2rpx solid #ededed;
 }
 .teamwork .teamwork-item image{
  width:90rpx;
  height:90rpx;
 }
 .teamwork .teamwork-item text{
   margin-top: 20rpx;
 }